        "mainPasswordInit":true,
        "appendToChat":true,
        "showGames":true,
        "showAverages":true,
        "getOneGameReport":true,
        "gamePlayersToStr":true,
        "initCardTable":true,
        "gamePlayersToDiv":true,
        "initOtherPlayers":true,
        "initRuleList":true,
        "initSpeedBar":true,
        "deleteIntervaller":true,
        "getCurrentRoundInd":true,
        "createPromiseTable":true,
        "createScoreboard":true,
        "getSelectValue":true,
        "removeCardEvents":true,
        "disableButtonsByClass":true,
        "emptyElementById":true,
        "emptyElementByClass":true,
        "removeElementById":true,
        "removeEventByClass":true,
        "createElementWithIdAndClasses":true,
        "removeClassByClass":true,
        "playedGamesGraph":true,
        "avgKeepPercentageGraph":true,
        "avgPointsGraph":true,
        "totalPointsGraph":true,
        "totalWinsGraph":true,
        "winPercentagesGraph":true,
        "scoreGraph":true,
        "avgPercentagePointsGraph":true,
        "cardsInHandGraph":true
